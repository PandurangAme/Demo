package Demo;

public class ArmstrongNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int  N=371;
		int temp=N;
		double res=0;
		int remainder=0;
		while(N!=0) {
			remainder=temp%10;
			res=res+Math.pow(remainder, 3);
			temp=temp/10;
		}
		if(res==N) {
			System.out.println("Armstrong Number"+res);
		}

	}

}
