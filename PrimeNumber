package Demo;

public class PrimeNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=29;
		boolean flag=false;
		if (n==0||n==1) {
			flag=true;
		
		}else {
		for (int i=2;i<=n/2;i++) {
			if (n%i==0) {
				//System.out.println("Not prime Number"+i);
				flag=true;
				break;
			}
		}
		if (!flag) {
			System.out.println("prime number"+n);
		}
		else {
			System.out.println("Not prime number"+n);
		}
		
		}
	}

}
